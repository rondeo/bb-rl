  "scripts": {
    "start": "set REACT_APP_DEVELOPMENT=true&&react-scripts start",
    "startLocalProdApi": "set REACT_APP_API=local_prod&&react-scripts start",
    "startLocalDevApi": "set REACT_APP_API=local&&react-scripts start",
    "startDevApi": "set REACT_APP_API=dev&&react-scripts start",
    "start-https": "set HTTPS=true&&react-scripts start",
    "startExpress": "node express",
    "build": "react-scripts build",
    "serve": "serve -s build",
    "buildAndUpload": "npm run sitemap & react-scripts build & gulp upload",
    "buildAndUploadDev": "set REACT_APP_DEVELOPMENT=true&&set REACT_APP_API=dev&&npm run sitemap & react-scripts build & gulp uploadDev",
    "upload": "gulp upload",
    "uploadDev": "gulp uploadDev",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sitemap": "babel-node utils/sitemap-generator.js",
    "styles": "node-sass --source-map src src/App.scss src/App.css && node-sass --recursive --include-path ./src/views  --source-map src/views src/views -o src/views && node-sass --recursive --include-path ./src/components --source-map src/components src/components -o src/components",
    "extract:messages": "react-intl-cra src/**/*.{js,jsx} -o src/i18n/messages.json",
    "manage:translations": "node ./src/i18n/translationRunner.js"
  },
  "browserslist": [
    "defaults"
  ]
}
